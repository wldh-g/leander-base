{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "esnext",
    ],
    "allowJs": true,
    "baseUrl": ".",
    "outDir": "run",
    "allowSyntheticDefaultImports": true,
    "keyofStringsOnly": true,
  },
  "include": [
    "./src/**/*",
  ],
  "exclude": [
    "./node_modules/**/*",
  ]
}
