{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es2018",
    "lib": [
      "es2018",
      "dom",
    ],
    "allowJs": true,
    "baseUrl": "./src",
    "outDir": "./run",
    "allowSyntheticDefaultImports": true,
    "keyofStringsOnly": false
  },
  "include": [
    "./src/**/*",
  ],
  "exclude": [
    "./node_modules/**/*",
  ]
}
